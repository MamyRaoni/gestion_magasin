{% extends 'base.html.twig' %}

{% block title %}CommandeProducts index{% endblock %}

{% block body %}
    <h1>CommandeProducts index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Quantity</th>
                <th>UnitPrice</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for commande_product in commande_products %}
            <tr>
                <td>{{ commande_product.id }}</td>
                <td>{{ commande_product.quantity }}</td>
                <td>{{ commande_product.unitPrice }}</td>
                <td>
                    <a href="{{ path('app_commande_products_show', {'id': commande_product.id}) }}">show</a>
                    <a href="{{ path('app_commande_products_edit', {'id': commande_product.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="4">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_commande_products_new') }}">Create new</a>
{% endblock %}
